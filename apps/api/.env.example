# Sikka API Configuration
SIKKA_API_URL=https://api.sikkasoft.com/v2
SIKKA_API_KEY=your_api_key_here
SIKKA_PRACTICE_ID=your_practice_id_here

# Retell API Configuration
RETELL_API_URL=https://api.retell.ai/v1
RETELL_API_KEY=your_retell_api_key_here
RETELL_WEBHOOK_SECRET=your_retell_webhook_secret_here

# API Server Configuration
PORT=3001
NODE_ENV=development

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Timeouts
REQUEST_TIMEOUT_MS=30000
CONNECT_TIMEOUT_MS=5000

# Retry Configuration
MAX_RETRIES=3
INITIAL_RETRY_DELAY_MS=1000
MAX_RETRY_DELAY_MS=5000

# Cache Configuration
ELIGIBILITY_CACHE_TTL_MS=86400000
BENEFITS_CACHE_TTL_MS=43200000
CACHE_MAX_SIZE=1000