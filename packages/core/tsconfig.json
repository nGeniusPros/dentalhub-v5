{
  "extends": "../tsconfig/base.json",
  "compilerOptions": {
  "lib": ["ES2022", "DOM", "DOM.Iterable"],
  "moduleResolution": "Bundler",
  "types": ["vite/client"],
  "strict": true,
  "outDir": "./dist",
  "rootDir": "./src",
  "composite": true,
  "diagnostics": true,
  "traceResolution": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}